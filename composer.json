{
    "name": "snuzi/engineering-blogs-aggregator",
    "description": "Engineering blogs aggregator",
    "type": "project",
    "authors": [
        {
            "name": "snuzi"
        }
    ],
    "require": {
        "symfony/console": "^5.2",
        "meilisearch/meilisearch-php": "0.19.2",
        "vlucas/phpdotenv": "^5.3",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "fabpot/goutte": "^4.0",
        "snuzi/awesome-tech-blogs": "^1.0",
        "laminas/laminas-feed": "^2.13",
        "laminas/laminas-http": "^2.14",
        "wa72/url": "^0.7.1"
    },
    "autoload": {
        "psr-4": { "EngBlogs\\": "src/" }
    },
    "repositories":[
        {              
            "type": "package",
            "package": {
              "name": "snuzi/awesome-tech-blogs",
              "version": "1.0.0",
              "source": {
                "url": "https://github.com/snuzi/awesome-tech-blogs.git",
                "type": "git",
                "reference": "main"
              }
            }
        }
    ]
}
